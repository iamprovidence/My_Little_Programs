<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>Custom.StyleCop</id>
		<version>$version$</version>
		<description>Custom Code Style definition</description>
		<authors>Taras Kizlo</authors>
		<owners>Taras Kizlo</owners>
		<projectUrl>https://github.com/iamprovidence</projectUrl>
		<icon>images\icon.png</icon>
		<license type="expression">MIT</license>
		<tags>StyleCop, CodeAnalysis</tags>
		<dependencies>
			<dependency id="StyleCop.Analyzers" version="1.1.118" />
		</dependencies>
	</metadata>

	<files>
		<file src="Custom.StyleCop.props" target="build" />
		
		<file src="stylecop.json" target="" />
		<file src="StyleCop.ruleset" target="" />

		<file src="images\icon.png" target="images" />
		
		<file src="analyzers\*.dll" target="analyzers" />
		
		<file src="tools\*.ps1" target="tools" />
	</files>
</package>
